#include <iostream>
using namespace std;

int main() {
    int n;

    cout << "Masukkan jumlah angka dalam deret: ";
    cin >> n;

    int max, num;
    cout << "Masukkan angka ke-1: ";
    cin >> max;

    for (int i = 2; i <= n; i++) {
        cout << "Masukkan angka ke-" << i << ": ";
        cin >> num;

        if (num > max) {
            max = num;
        }
    }

    cout << "Bilangan terbesar dalam deret adalah: " << max << endl;

    return 0;
}
